{
	"name": "eract",
	"version": "1.1.0",
	"private": true,
	"dependencies": {
		"bootstrap": "^4.0.0-alpha.6",
		"draft-js": "^0.10.1",
		"react": "^15.6.1",
		"react-dom": "^15.6.1",
		"react-draft-wysiwyg": "^1.10.4",
		"react-icons": "^2.2.5",
		"react-notification-system": "^0.2.14",
		"react-router": "^3.0.5",
